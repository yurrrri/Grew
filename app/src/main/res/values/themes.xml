<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Grew.white" parent="Theme.Design.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/grew_green</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>
        <!-- Customize your theme here. -->
        <item name="background">@color/white</item>
    </style>

    <style name="Theme.Grew.green" parent="Theme.Design.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/grew_green</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/grew_green</item>
        <!-- Customize your theme here. -->
        <item name="background">@color/white</item>
    </style>

    <style name="titleText">
        <item name="android:textSize">30dp</item>
        <item name="fontFamily">@font/nanum_squared_round_extra_bold</item>
    </style>
</resources>